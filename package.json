{
  "name": "gitlab-mcp-server",
  "version": "1.0.0",
  "description": "一个可扩展的GitLab MCP服务器，支持多种GitLab操作和AI驱动的代码分析",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "gitlab-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch & node --watch dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test-connection": "npm run build && node dist/index.js --test-connection",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "gitlab",
    "mcp",
    "model-context-protocol",
    "merge-request",
    "ai",
    "cursor",
    "typescript",
    "devtools"
  ],
  "author": "Your Name",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@gitbeaker/node": "^35.8.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.9.0",
    "zod": "^3.23.8",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
